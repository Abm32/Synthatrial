{
  "version": 2,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api.py"
    }
  ],
  "env": {
    "GOOGLE_API_KEY": "@google_api_key",
    "GEMINI_MODEL": "gemini-2.5-flash",
    "GEMINI_TEMPERATURE": "0.3"
  }
}
